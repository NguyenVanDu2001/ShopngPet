@model ShopThoiTrang.Models.Mcontact
@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Wrapper. Contains page content -->
    <div class="card card-title bg-white p-1">
        <div class="row">
            <div class="col-md-6 "> <h5 class="pl-2 mt-2 text-blues">Quản lý liên hệ</h5> </div>
            <div class="col-md-6 m-0">
                <ol class="breadcrumb bg-white float-sm-right m-0">
                    <li class="breadcrumb-item m-0"><a href="~/Admin">Trang chủ</a></li>
                    <li class="breadcrumb-item active m-0">Quản lý liên hệ</li>
                </ol>
            </div>
        </div>
    </div>
    <!-- /.content-header -->
    <section class="pt-2 container-fluid ">
        <div class="container pt-2 bg-white card ">
            <div class="col-md-12 ">
                <div class="row pl-3 text-center">
                    <h3> Tiêu đề: @Html.DisplayFor(model => model.title)  </h3>
                    <br />

                </div>
                <span>Tên:  <span class="text-cam">@Html.DisplayFor(model => model.fullname)</span></span>
                <p class="pt-1"> Message: @Html.DisplayFor(model => model.detail)</p>
           
            </div>
        </div>
    </section>
    <script>
        $("#contact").addClass("active");
    </script>
